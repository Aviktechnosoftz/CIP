/*Dilogue box style*/

#dialogoverlay{
  display: none;
  opacity: .8;
  position: fixed;
  top: 0px;
  left: 0px;
  background: #FFF;
  width: 100%;
  z-index: 10;
  
}
#dialogbox{
  display: none;
  position: fixed;
  background: #000;
  border-radius:7px; 
  width: 30vw;
  z-index: 10;
  text-align: left;
    
}
#dialogbox > div{ background:#FFF; margin:3px; }
#dialogbox > div > #dialogboxhead{ background: lightgray; font-size:15px; padding:10px;border-bottom: 2px solid black;  }
#dialogbox > div > #dialogboxbody{ background: lightgray; padding:20px; }
#dialogbox > div > #dialogboxfoot{ background: lightgray; padding:10px; text-align:right; }